<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bitchair.mapper.PowerMapper">
    <select id="findByUsername" resultType="com.example.bitchair.entity.Power" parameterType="String">
        SELECT
            *
        FROM
            power
        WHERE
            username =#{username}
    </select>
</mapper>